{
    "name": "jitume-lab/workstation-reservation-system",
    "description": "A web-based workstation reservation system for Jitume Lab at STVC.",
    "type": "project",
    "require": {
        "php": "^7.4 || ^8.0",
        "vlucas/phpdotenv": "^5.4",
        "illuminate/database": "^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}